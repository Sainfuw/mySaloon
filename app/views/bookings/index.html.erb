<p id="notice"><%= notice %></p>

<h1>Bookings</h1>

<table>
  <thead>
    <tr>
      <th>Formula</th>
      <th>Comment</th>
      <th>Date</th>
      <th>Start time</th>
      <th>End time</th>
      <th>Status</th>
      <th>User</th>
      <th>Author</th>
      <th>Customer</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bookings.each do |booking| %>
      <tr>
        <td><%= booking.formula %></td>
        <td><%= booking.comment %></td>
        <td><%= booking.date %></td>
        <td><%= booking.start_time %></td>
        <td><%= booking.end_time %></td>
        <td><%= booking.status %></td>
        <td><%= booking.user %></td>
        <td><%= booking.author %></td>
        <td><%= booking.customer %></td>
        <td><%= link_to 'Show', booking %></td>
        <td><%= link_to 'Edit', edit_booking_path(booking) %></td>
        <td><%= link_to 'Destroy', booking, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Booking', new_booking_path %>
